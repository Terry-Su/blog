{
  "name": "blog",
  "version": "1.0.0",
  "description": "",
  "main": ".eslintrc.js",
  "scripts": {
    "start": "cross-env DEV=true nodemon --verbose -e ts,tsx,md",
    "build": "ts-node --project tsconfig.node.json -r tsconfig-paths/register server",
    "publish": "npm run build && npm run tsnode scripts/publishToSite",
    "i18n": "npm run tsnode scripts/i18n",
    "other": "npm run tsnode scripts/buildContentsOnOtherPlatforms",
    "tsnode": "cross-env TS_NODE_PROJECT=tsconfig.node.json ts-node",
    "tmp": "npm run tsnode scripts/build server"
  },
  "nodemonConfig": {
    "watch": [
      "tsblog.config.ts",
      "server.ts",
      "i18nContents.ts",
      "../tsblog/src/**/*"
    ],
    "exec": "ts-node --project tsconfig.node.json -r tsconfig-paths/register server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Terry-Su/blog.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Terry-Su/blog/issues"
  },
  "homepage": "https://github.com/Terry-Su/blog#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/standalone": "^7.4.3",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@mdx-js/loader": "^0.20.3",
    "@mdx-js/mdx": "^0.20.3",
    "@types/babel-types": "^7.0.6",
    "@types/fs-extra": "^5.0.5",
    "@types/node": "^11.10.4",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.0.1",
    "@types/styled-components": "^4.1.11",
    "@types/webpack-env": "^1.13.9",
    "cache-loader": "^2.0.1",
    "chokidar": "^2.1.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "fork-ts-checker-webpack-plugin": "^1.0.3",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "html-to-text": "^4.0.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "prismjs": "^1.16.0",
    "raw-loader": "^2.0.0",
